{
  "name": "speedy-recovery-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fhirclient": "^0.1.15",
    "moment": "latest",
    "react": "^16.7.0",
    "react-big-calendar": "latest",
    "react-chat-elements": "^10.2.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "semantic-ui-react": "^0.84.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prepare": "patch-package",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write src/**/*.{js,json,css}",
    "format:check": "prettier --list-different src/**/*.{js,json,css}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "jest-localstorage-mock": "^2.4.0",
    "patch-package": "^6.0.2",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.16.3"
  }
}
